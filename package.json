{
  "name": "energe-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "export": "next build && next export",
    "storybook": "start-storybook -p 9001 -c .storybook -s ./static"
  },
  "dependencies": {
    "@material-ui/core": "^4.5.2",
    "@material-ui/icons": "^4.5.1",
    "@types/styled-components": "^4.1.19",
    "dayjs": "^1.8.16",
    "dotenv": "^8.1.0",
    "i18next": "^19.0.1",
    "material-table": "^1.53.0",
    "next": "9.0.7",
    "react": "16.10.2",
    "react-dom": "16.10.2",
    "react-i18next": "^11.2.5",
    "react-virtualized": "^9.21.1",
    "recharts": "^1.8.5",
    "styled-components": "^4.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@storybook/addon-actions": "^5.2.8",
    "@storybook/addon-info": "^5.2.8",
    "@storybook/addon-knobs": "^5.2.8",
    "@storybook/addon-notes": "^5.2.8",
    "@storybook/addon-storysource": "^5.2.8",
    "@storybook/addon-viewport": "^5.2.8",
    "@storybook/react": "^5.2.8",
    "@types/dotenv": "^6.1.1",
    "@types/node": "^12.7.11",
    "@types/react": "^16.9.5",
    "@types/react-dom": "^16.9.3",
    "@types/recharts": "^1.8.2",
    "@visuallylab/tslint-config-frontend": "^1.0.13",
    "@zeit/next-bundle-analyzer": "^0.1.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-styled-components": "^1.10.6",
    "dotenv-webpack": "^1.7.0",
    "husky": "^3.0.8",
    "lint-staged": "^9.4.1",
    "next-images": "^1.2.0",
    "prettier": "^1.18.2",
    "react-docgen-typescript-loader": "^3.6.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn run lint && lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "jsxSingleQuote": false,
    "trailingComma": "all"
  }
}
